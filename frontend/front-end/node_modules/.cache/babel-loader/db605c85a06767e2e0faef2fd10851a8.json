type }}</div>\r\n        <v-list-item-title class=\"text-h5 mb-1\" v-if=\"editMode\">\r\n          <v-text-field v-model=\"value.name\"></v-text-field>\r\n        </v-list-item-title>\r\n        <v-list-item-title class=\"text-h5 mb-1\" @click=\"toggleEditMode\" v-else>\r\n          {{ value.name }}\r\n        </v-list-item-title>\r\n        <v-list-item-subtitle @click=\"toggleEditMode\"\r\n          >에너지는 {{ value.energy }}이고 외모지수는\r\n          {{ value.appearance }}입니다.</v-list-item-subtitle\r\n        >\r\n      </v-list-item-content>\r\n\r\n      <v-list-item-avatar tile size=\"80\" color=\"grey\"></v-list-item-avatar>\r\n    </v-list-item>\r\n\r\n    <v-card-actions>\r\n      <v-btn outlined rounded text @click=\"eat\"> Feed </v-btn>\r\n      <v-btn outlined rounded text @click=\"sleep\"> Sleep </v-btn>\r\n      <slot name=\"buttons\"></slot>\r\n    </v-card-actions>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Pet\",\r\n\r\n  props: {\r\n    value: Object,\r\n    editMode: Boolean,\r\n  },\r\n\r\n  methods: {\r\n    eat() {\r\n      if (this.$parent.eat) {\r\n        this.$parent.eat();\r\n      } else {\r\n        this.value.energy += 2;\r\n      }\r\n\r\n      this.$emit(\"change\", this.value);\r\n    },\r\n    sleep() {\r\n      if (this.$parent.sleep) {\r\n        this.$parent.sleep();\r\n      } else {\r\n        this.value.energy += 2;\r\n      }\r\n\r\n      this.$emit(\"change\", this.value);\r\n    },\r\n    toggleEditMode() {\r\n      this.editMode = !this.editMode;\r\n      this.$emit(\"change\", this.value);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}