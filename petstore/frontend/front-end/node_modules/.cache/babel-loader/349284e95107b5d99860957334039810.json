kBAAA,OAAA,EAAA;AAAA,oCAAA;AAAA;AAHA,iBAAA,CADA;;AAAA;AAOA,gBAAA,MAAA,CAAA,OAAA,GAPA,CASA;AACA;AACA;AACA;AACA;AACA;;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KAtCA;AAwCA,IAAA,OAxCA,qBAwCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CACA,IAAA,GAAA,CAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QADA,CADA;;AAAA;AACA,gBAAA,QADA;;AAKA,oBAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;;AACA,kBAAA,MAAA,CAAA,KAAA,CAAA,OAAA,EAAA,MAAA,CAAA,KAAA;AACA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAjDA;AAmDA,IAAA,cAnDA,4BAmDA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,KAAA,KAAA;AACA;AAtDA;AARA,CAAA","sourcesContent":["<template>\n  <v-card class=\"mx-auto\" max-width=\"344\" outlined>\n    <v-list-item three-line>\n      <v-list-item-content>\n        <div class=\"text-overline mb-4\">{{ value.type }}</div>\n        <v-list-item-title class=\"text-h5 mb-1\" v-if=\"editMode\">\n          <v-text-field v-model=\"value.name\"></v-text-field>\n        </v-list-item-title>\n        <v-list-item-title class=\"text-h5 mb-1\" @click=\"toggleEditMode\" v-else>\n          {{ value.name }}\n        </v-list-item-title>\n        <v-list-item-subtitle @click=\"toggleEditMode\"\n          >에너지는 {{ value.energy }}이고 외모지수는\n          {{ value.appearance }}입니다.</v-list-item-subtitle\n        >\n      </v-list-item-content>\n\n      <v-list-item-avatar tile size=\"80\" color=\"grey\"></v-list-item-avatar>\n    </v-list-item>\n\n    <v-card-actions>\n      <v-btn outlined rounded text @click=\"eat\"> Feed </v-btn>\n      <v-btn outlined rounded text @click=\"sleep\"> Sleep </v-btn>\n      <slot name=\"buttons\"></slot>\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\nconst axios = require(\"axios\").default;\n\nexport default {\n  name: \"Pet\",\n\n  props: {\n    value: Object,\n    editMode: Boolean,\n  },\n\n  methods: {\n    async eat() {\n      await axios.request({\n        method: \"PUT\",\n        url: new URL(this.value._links.feed.href).pathname,\n        headers: { \"Content-Type\": \"application/json\" },\n      });\n\n      this.refresh();\n\n      // if (this.$parent.eat) {\n      //   this.$parent.eat();\n      // } else {\n      //   this.value.energy += 2;\n      // }\n      // axios.request({\n      //   method: \"PATCH\",\n      //   url: new URL(this.value._links.self.href).pathname,\n      //   headers: { \"Content-Type\": \"application/json\" },\n      //   data: this.value,\n      // });\n      // this.$emit(\"change\", this.value);\n    },\n    async sleep() {\n      await axios.request({\n        method: \"PUT\",\n        url: new URL(this.value._links.sleep.href).pathname,\n        headers: { \"Content-Type\": \"application/json\" },\n      });\n\n      this.refresh();\n\n      // if (this.$parent.sleep) {\n      //   this.$parent.sleep();\n      // } else {\n      //   this.value.energy += 2;\n      // }\n      // this.$emit(\"change\", this.value);\n    },\n\n    async refresh() {\n      var response = await axios.get(\n        new URL(this.value._links.self.href).pathname\n      );\n\n      if (response) {\n        this.value = response.data;\n        this.$emit(\"input\", this.value);\n      }\n    },\n\n    toggleEditMode() {\n      this.editMode = !this.editMode;\n      this.$emit(\"change\", this.value);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}